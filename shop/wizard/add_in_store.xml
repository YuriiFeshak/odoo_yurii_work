<?xml version="1.0"?>
<odoo>
    <record id="view_add_in_store_wizard_form" model="ir.ui.view">
        <field name="name">add.in.store.form</field>
        <field name="model">add.in.store</field>
        <field name="arch" type="xml">
            <form string="Add in store">
                <sheet>
                    <group>
<!--                        <field name="stock_id" invisible="1"/>-->
                        <field name="up_stock_in" readonly="0"/>
                        <field name="up_stock_out"/>
                        <field name="up_left_field" readonly="1"/>
                    </group>
                    <footer>
<!--                        <button string="Add in store" name="action_add_value"  type="object"/>-->
                        <button string="Add in store" class="btn-primary" special="save"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>

